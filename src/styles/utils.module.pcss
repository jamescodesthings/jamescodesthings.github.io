.contain {
  @apply container m-auto relative;
  composes: pb from 'spacing.module.pcss';
  composes: px-half from 'spacing.module.pcss';
  composes: first-pt from 'spacing.module.pcss';

  @apply print:!m-0 print:!p-0 print:!w-full print:!max-w-full;
}

.contain.screen {
  @apply h-screen;
}

.hr {
  @apply border-type dark:border-type-dark;
}
